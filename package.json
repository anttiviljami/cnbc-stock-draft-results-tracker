{
  "name": "cnbc-stock-draft-results-tracker",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost:8000/"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.5",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "yahoo-finance": "^0.3.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "react-dev-server": "cross-env PORT=${DEV_PORT} react-scripts start",
    "dev": "npm-run-all -p watch-server watch-css react-dev-server",
    "watch-server": "nodemon -w src/api server.js",
    "start-server": "node server.js",
    "start": "npm run start-server",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom --silent",
    "eject": "react-scripts eject",
    "lint": "eslint **/*.{js,jsx}"
  },
  "devDependencies": {
    "nodemon": "^1.17.4"
  }
}
